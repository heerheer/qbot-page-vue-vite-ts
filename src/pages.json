{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "功能手册"
      }
    },
    {
      "path": "pages/info/index",
      "style": {
        "navigationBarTitleText": "关于"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app-vite-vue3-ts",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#efeff4"
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#4F9BFA",
    "borderStyle": "none",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/tabbar/functions.png",
        "selectedIconPath": "static/tabbar/functions-s.png",
        "text": "功能手册"
      },{
        "pagePath": "pages/info/index",
        "iconPath": "static/tabbar/infos.png",
        "selectedIconPath": "static/tabbar/infos-s.png",
        "text": "关于"
      }
    ]
  }
}
